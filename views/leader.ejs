<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Ranking</title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="/assets/materialize.min.css">
    <style>
    body{
       
       
        background: url('/assets/astronomy-1867616_1280.jpg') no-repeat 50%50%;
        background-size: cover;
    }
    #t{
        opacity: 0.8;
        text-align: center;
    }
    #an{
        opacity: 0.7;
        background-color: black;
        margin:10px ;
      padding:10px 20px;;
      border-radius: 13px;
    }
    .activet{
        background-color:grey;
    }
    #an:hover{
        opacity: 0.85;
    }
    .table{
        background-color: black;
        opacity: .7;
        margin:0 auto;
        padding:0;
    }
    .table:hover{
        opacity:.85;
    }
    footer{
        height:80px;
    }
   
    </style>
</head>
<body>
    <div class="container center">
       <img class="responsive-img"src="./assets/Kalpana.png" alt="Kalpana">     

       <nav class="nav-wrapper black " id="t">
            <div class="nav-content">
                
                        <a class="sidenav-trigger brand-logo  hide-on-large-only" href="#!" data-target="mobile-links"><i class="material-icons right">menu</i></a>
                    <ul id="" class="center hide-on-med-and-down ">
                                
                        <li><a href="/main"><i class="material-icons left">home</i>Home</a></li>
                            <li class="activet"><a href="/leader"><i class="material-icons left">stars</i>Ranking</a></li>
                            <li><a href="/about"><i class="material-icons left">chat</i>About</a></li>
                            
                        </ul>
                        <ul id="" class="right hide-on-med-and-down ">
                                <li ><a target="_blank" href="https://www.facebook.com/ieeepesitbsc"><i class="material-icons left">public</i>Fb Page</a></li>
                                
                                <li><a href="/logout"><i class="material-icons left">arrow_back</i>Logout</a></li>
                            </ul>
                            
                            
            </div>
    
        </nav>
        <ul id="mobile-links" class="sidenav center hide-on-large-only">
                                
                <li><a href="/main"><i class="material-icons left">home</i>Home</a></li>
                    <li class="activet"><a href="/leader"><i class="material-icons left">stars</i>Ranking</a></li>
                    <li><a href="/about"><i class="material-icons left">chat</i>About</a></li>
                    <li ><a target="_blank" href="https://www.facebook.com/ieeepesitbsc"><i class="material-icons left">public</i>Fb Page</a></li>
                    <li><a href="/logout"><i class="material-icons left">arrow_back</i>Logout</a></li>
                </ul>
    
        <div class="row">
            <br>
        <div class="table col l12 ">
            <table style="width:100%"class=" highlight white-text bordered">
                    <thead class="">
                      <tr>
                          <th>Rank</th>
                          <th>UserName</th>
                          <th>College</th>
                          <th>Level</th>
                          <th>Time</th>

                      </tr>
                    </thead>
            
                    <tbody>
                        <% var i=0;
                            for(i=0;i<data.length;i++){ %>
                            <tr>
                                    <td ><%=i+1%></td>
                                    <td><%=data[i].username%></td>
                                    <td><%=data[i].college%></td>
                                    <td><%=data[i].level%></td>
                                    <td><%=data[i].date.getHours()%>:<%=data[i].date.getMinutes()%>:<%=data[i].date.getSeconds()%></td>
                                  </tr>
                                  <%}%>
                                  
                                      

                    </tbody>
                  </table>

    
</div>
</div>
    </div>
    <br><br>
    <div class="container ">
    <footer class="page-footer " id="an">
            
              <div class="row center">
                <div class="col  s12">
                  <h6 class="white-text">Developed By-</h6>
                  <p class="grey-text text-lighten-4"> <a  target="_blank"href="http://www.github.com/jdmallige">Jagdish</a></p>
                </div>
                
              </div>
            </footer>
        </div>
    </body>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
    <script>
   $(document).ready(function(){
    $('.sidenav').sidenav();
  });
        
    </script>
<script src="/assets/materialize.min.js"></script>

    </html>
